{
  "name": "starter-file",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch": "node-sass sass/main.scss dist/css/style.prefix.css -w",
    "server:prod": "live-server --port=8181 --browser=chrome",
    "start": "concurrently \"npm run server:prod\" \"npm run watch-dev\"",
    "compile": "node-sass sass/main.scss dist/css/style.comp.css -w",
    "concat": "concat -o dist/css/style.concat.css dist/css/all.css dist/css/style.comp.css",
    "prefix": "postcss dist/css/style.concat.css --use autoprefixer -o dist/css/style.prefix.css",
    "build": "npm-run-all compiler:sass concat:css prefix:css"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.7.5",
    "concat": "^1.0.3",
    "gh-pages": "^2.2.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0"
  },
  "dependencies": {
    "all": "0.0.0",
    "concurrently": "^5.1.0"
  }
}
